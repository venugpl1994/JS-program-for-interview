function abc(str){
let flag=0, a=str.length
for(let i=0;i<a;i++){
if(str[i]!==str[a-i-1]){
	flag=1;
	break;
}
}
if(flag==1){
	console.log("it is nt a palindrom");
}
else{
console.log("it is  a palindrom");
}
}
abc("maam");
