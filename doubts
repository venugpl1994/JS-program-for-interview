div.relative {
    position: absolute;
    width: 400px;
    height: 200px;
    right: 100px;
    bottom:100px;
    
    border: 3px solid #73AD21;
} 

div.absolute {
    position: absolute;
    top: 80px;
    right: 0;
    width: 200px;
    height: 100px;
    border: 3px solid #73AD21;
}
