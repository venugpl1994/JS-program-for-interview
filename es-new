----------------------------------------------------------
comma is used to skip the element
-----------------------------------------------------------
let [a,,,,,,,,,c,...d]=[1,2,3,4,5,6,7,8,9,0,9,8,7]
console.log(a);
console.log(c);
console.log(...d);
------------------------------------------------------------
Distructure Assignment
let [one, two] = [1, 2];
let {three, four} = {three: 3, four:  4};
console.log(one, two, three, four); 
-------------------------------------------------------------
spread operator
var a = [1, 2];
var b = [3, 4];
var c = [...a,...b]
console.log(c);
------------------------------------------------------------------
spread operator
function getSum(x, y, z){
    console.log(x+y+z);
}
getSum(...[10,20,30]);
-----------------------------------------------------------------------
Rest operator
function numbers(x, y, ...z){
    console.log(x, y, z);
}
numbers(1, 2, 3, 4, 5, 6);
-----------------------------------------------------------------------

